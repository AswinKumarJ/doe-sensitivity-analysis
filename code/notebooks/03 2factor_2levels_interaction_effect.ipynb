    "import pandas as pd\n",
    "import csv\n",
    "df=pd.read_csv('/2factor_2levels.csv')\n",
    "print(df);\n",
    "factor1=df['factor 1'].tolist()\n",
    "factor2=df['factor 2'].tolist()\n",
    "output=df['output 1 (units)'].tolist()\n",
    "print(factor1,factor2,output);"
   ]
  },
