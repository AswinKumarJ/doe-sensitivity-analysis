    "def threeinteff(f1,f2,f3,op):\n",
    "    effect_lc=0\n",
    "    effect_hc=0\n",
    "    effect=0\n",
    "    tot_high_values=len(op)/2
    "    for i in range(len(op)):\n",
    "        if f3[i]==min(f3):\n",
    "            if f2[i]==min(f2):\n",
    "                if f1[i]==max(f1):\n",
    "                    effect_lc=effect_lc-op[i]\n",
    "                elif f1[i]==min(f1):\n",
    "                    effect_lc=effect_lc+op[i]\n",
    "            elif f2[i]==max(f2):\n",
    "                if f1[i]==max(f1):\n",
    "                    effect_lc=effect_lc+op[i]\n",
    "                elif f1[i]==min(f1):\n",
    "                    effect_lc=effect_lc-op[i]\n",
    "        elif f3[i]==max(f3):\n",
    "            if f2[i]==min(f2):\n",
    "                if f1[i]==max(f1):\n",
    "                    effect_hc=effect_hc-op[i]\n",
    "                elif f1[i]==min(f1):\n",
    "                    effect_hc=effect_hc+op[i]\n",
    "            elif f2[i]==max(f2):\n",
    "                if f1[i]==max(f1):\n",
    "                    effect_hc=effect_hc+op[i]\n",
    "                elif f1[i]==min(f1):\n",
    "                    effect_hc=effect_hc-op[i]\n",
